{
    "tags": {
        "allowUnknownTags": true
    },
    "source": {
        "include": "src",
        "includePattern": ".+(\\.js)$",
        "excludePattern": "(^|\\/|\\\\)_"
    },
    "templates": {
        "cleverLinks": false,
        "monospaceLinks": false
    },
    "opts": {
        "destination": "jsdocs",
        "recurse": true
    }
}